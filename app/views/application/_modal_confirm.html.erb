
<div class="modal fade" id="modal_confirm" tabindex="-1" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title"></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <p></p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-outline-secondary cancel" data-bs-dismiss="modal">
          <%= t(:cancel, scope: :actions) %>
        </button>
        <button type="button" class="btn btn-primary confirm"  data-bs-dismiss="modal">
          <%#= t(:confirm, scope: :actions) %>
        </button>
      </div>
    </div>
  </div>
</div>
