<% row = j render partial: 'location', formats: :html, locals: { location: @location, hide: true } %>
<%= turbo_stream.update 'run-script', partial: 'script_row_update', locals: { row: row, id: "location_#{@location.id}" } %>
<%= turbo_stream.update turbo_frame_id, partial: 'script_close_modal_form_entity' %>
<%= turbo_stream.update 'list-total', body: '' %>
<%= turbo_stream.replace 'flash-messages', partial: 'partial_flash_messages', locals: { trigger: true } %>