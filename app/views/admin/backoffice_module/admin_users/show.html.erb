<%= turbo_frame_tag turbo_frame_id do %>
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header">
        <h5 id="modal-title" class="modal-title"><%= AdminUser.model_name.human %></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">

        <div class="row">
          <div class="col-md-4">
            <%= custom_text_field(AdminUser.human_attribute_name(:last_name), @admin_user.last_name) %>
          </div>
          <div class="col-md-4">
            <%= custom_text_field(AdminUser.human_attribute_name(:first_name), @admin_user.first_name) %>
          </div>
          <div class="col-md-4">
            <%= custom_text_field(AdminUser.human_attribute_name(:email), @admin_user.email) %>
          </div>
        </div>



      </div>
    </div>
  </div>
  <%= render "script_open_modal_form_entity" %>
<% end %>

